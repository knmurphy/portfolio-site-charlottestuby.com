{
  "name": "charlottestuby-cms",
  "description": "Charlotte stuby's online portfolio",
  "version": "1.1.3",
  "author": "Lucas Vogel <contact@vogelino.com> (http://vogelino.com)",
  "dependencies": {
    "gatsby": "^2.20.35",
    "gatsby-image": "^2.3.5",
    "gatsby-plugin-manifest": "^2.4.35",
    "gatsby-plugin-netlify": "^2.2.4",
    "gatsby-plugin-netlify-cms": "^4.2.5",
    "gatsby-plugin-preload-fonts": "^1.2.28",
    "gatsby-plugin-react-helmet": "^3.2.5",
    "gatsby-plugin-sharp": "^2.5.7",
    "gatsby-plugin-sitemap": "^2.4.17",
    "gatsby-plugin-stylus": "^2.3.13",
    "gatsby-remark-copy-linked-files": "^2.2.4",
    "gatsby-remark-images": "^3.2.6",
    "gatsby-remark-relative-images": "^0.3.0",
    "gatsby-source-filesystem": "^2.2.5",
    "gatsby-transformer-remark": "^2.7.5",
    "gatsby-transformer-sharp": "^2.4.7",
    "netlify-cms-app": "^2.9.6",
    "netlify-cms-media-library-cloudinary": "^1.3.2",
    "netlify-cms-media-library-uploadcare": "^0.5.2",
    "nib": "^1.1.2",
    "nuka-carousel": "^4.6.1",
    "prop-types": "^15.6.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-helmet": "^6.0.0",
    "react-markdown": "^5.0.0"
  },
  "keywords": [
    "gatsby",
    "netlify",
    "netlify cms",
    "portfolio",
    "artist",
    "swiss",
    "brussels",
    "textile"
  ],
  "homepage": "http://charlottestuby.com",
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "clean": "gatsby clean",
    "start": "npx serve public",
    "build": "npm run clean && npm run build-styles-cms && gatsby build && npm run build-styles-prod",
    "lint": "eslint src",
    "lint:fix": "eslint --fix \"{gatsby-*.js,src/**/*.js}\"",
    "dev": "npm run clean && (npm run watch-styles & gatsby develop)",
    "format": "prettier --write \"{gatsby-*.js,src/**/*.js}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-styles-prod": "stylus -u nib ./src/styles/index.styl -c -o ./public/styles.css",
    "build-styles-dev": "stylus -u nib ./src/styles/index.styl -o ./public/styles.css",
    "build-styles-cms": "stylus -u nib ./src/styles/index.styl -o ./src/cms/styles.css",
    "watch-styles": "npm run build-styles-dev -- -w",
    "preload-fonts": "gatsby-preload-fonts"
  },
  "devDependencies": {
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "prettier": "^2.1.2"
  }
}
