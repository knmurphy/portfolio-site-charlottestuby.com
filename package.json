{
  "name": "charlottestuby.com",
  "description": "Charlotte stuby's online portfolio",
  "version": "2",
  "author": "Lucas Vogel <contact@vogelino.com> (https://vogelino.com)",
  "main": "n/a",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "export": "npm run build && next export",
    "lint": "eslint .",
    "lint-fix": "npm run lint -- --fix"
  },
  "keywords": [
    "gatsby",
    "netlify",
    "netlify cms",
    "portfolio",
    "artist",
    "swiss",
    "brussels",
    "textile"
  ],
  "homepage": "https://charlottestuby.com",
  "license": "MIT",
  "dependencies": {
    "frontmatter-markdown-loader": "3.6.2",
    "gray-matter": "4.0.2",
    "gsap": "3.6.1",
    "netlify-cms-app": "2.14.32",
    "netlify-cms-media-library-cloudinary": "1.3.10",
    "netlify-cms-media-library-uploadcare": "0.5.10",
    "nuka-carousel": "4.7.7",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-markdown": "5.0.3"
  },
  "devDependencies": {
    "@babel/core": "7.13.13",
    "@babel/preset-react": "7.13.13",
    "babel-eslint": "10.1.0",
    "eslint": "7.22.0",
    "next": "10.0.9",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "5.1.2",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-commit": "lint-staged && npm test -- -o --coverage --watchAll=false --passWithNoTests"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
