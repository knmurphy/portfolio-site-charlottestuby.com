{
	"plugins": [
		"prettier",
		"react",
		"import"
	],
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended",
		"plugin:prettier/recommended",
		"plugin:import/recommended",
		"prettier"
	],
	"parserOptions": {
		"ecmaVersion": 2020,
		"sourceType": "module",
		"jsx": true
	},
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"rules": {
		"semi": ["error", "never"],
		"indent": ["error", "tab"]
	}
}
